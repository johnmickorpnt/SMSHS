<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MyAccount">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="64dp"
            android:text="My Account"
            android:textAlignment="center"
            android:textSize="34sp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/textView3"
            android:padding="24dp">

            <EditText
                android:id="@+id/lrnText"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_below="@+id/nameText"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="12dp"
                android:editable="false"
                android:inputType="textPersonName"
                android:text="LRN" />

            <EditText
                android:id="@+id/strandText"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_below="@+id/lrnText"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="12dp"
                android:editable="false"
                android:inputType="textPersonName"
                android:text="Strand" />

            <EditText
                android:id="@+id/gradeTextLabel"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_below="@+id/strandText"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="12dp"
                android:editable="false"
                android:inputType="textPersonName"
                android:text="Grade" />

            <EditText
                android:id="@+id/date_of_birth_text"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_below="@+id/gradeTextLabel"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="12dp"
                android:editable="false"
                android:inputType="textPersonName"
                android:text="Date of Birth" />

            <EditText
                android:id="@+id/nameText"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:editable="false"
                android:inputType="textPersonName"
                android:text="Name" />

            <EditText
                android:id="@+id/addressText"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_below="@+id/date_of_birth_text"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="12dp"
                android:editable="false"
                android:gravity="start|top"
                android:inputType="textMultiLine"
                android:text="Address" />

            <EditText
                android:id="@+id/contactText"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_below="@+id/addressText"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="12dp"
                android:editable="false"
                android:inputType="textPersonName"
                android:text="Contact Number" />

        </RelativeLayout>

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>